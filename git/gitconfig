[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[user]
	name = sidneyw
	email = sidneywijngaarde@gmail.com
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[push]
	default = current
[alias]
	br = branch
	ca = commit --amend
	cane = commit --amend --no-edit
	ci = commit
	cinv = commit --no-verify
	cmp = "!f() { git checkout master && git pull; }; f"
	co = checkout
	dh = diff HEAD
	fa = fetch --all
	pf = push -f
	pr = "!f() { git fetch origin refs/pull/$1/head:refs/remotes/origin/pr/$1 && git checkout FETCH_HEAD && git remote prune origin; }; f"
	rim = rebase -i upstream/master
	st = status
[core]
	editor = /usr/local/bin/nvim
	excludesfile = ~/.gitignore
